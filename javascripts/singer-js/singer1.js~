function gettingName(tipName)
{
		var iframe=document.createElement("iframe");
		iframe.src="http://www.last.fm/search?q=" + tipName + "&type=artist";
		iframe.width= 650;
		iframe.hight= 650;
		
		var element=document.getElementById("ittahelye");
		element.appendChild(iframe);
}

function gettingImage(tipName)
{
		var iframe=document.createElement("iframe");
		iframe.src="www.last.fm/music/" + tipName + "/+images";
		iframe.width= 650;
		iframe.hight= 650;
		
		var element=document.getElementById("ittahelye");
		element.appendChild(iframe);
}
